{
  "manifest_version": 3,
  "name": "Villantott",
  "version": "1.0",
  "description": "Collect best articles from Origo.hu title page",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  "content_scripts": [
    {
      "js": [
        "scripts/villantott.js"
      ],
      "matches": [
        "https://www.origo.hu/index.html*"
      ]
    }
  ]
}
