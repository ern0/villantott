{
  "manifest_version": 3,
  "name": "Villantott",
  "version": "1.1.3.21116",
  "description": "Collect softpron articles from Origo.hu title page",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  "host_permissions": [
    "https://www.origo.hu/"
  ],

  "content_scripts": [
    {
      "js": [
        "scripts/jquery-3.6.1.min.js",
        "scripts/villantott.js"
      ],
      "matches": [
        "https://www.origo.hu/index.html"
      ]
    }
  ]
  
}
